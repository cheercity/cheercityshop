<style>
	.dev-debug-toggle {
		background: #24292e;
		color: #fff;
		border: 0;
		padding: 6px 10px;
		margin: 6px;
		border-radius: 3px;
		font-size: 0.85rem;
		cursor: pointer
	}
	.dev-debug-pre {
		font-size: 0.85rem;
		line-height: 1.3;
	}
	.dev-debug-fixed {
		position: fixed;
		right: 1rem;
		bottom: 1rem;
		z-index: 9999;
	}
	.dev-debug-panel {
		position: fixed;
		right: 1rem;
		bottom: 1rem;
		z-index: 10000;
		background: rgba(0, 0, 0, 0.7);
		color: #fff;
		padding: 8px;
		border-radius: 6px;
		display: flex;
		gap: 6px;
		align-items: center
	}
	.dev-debug-panel .info {
		font-size: 0.8rem;
		opacity: 0.9;
		margin-left: 6px
	}
</style>
<script>
	(function () {
function togglePre(pre) {
if (! pre) 
return false;



pre.style.display = (pre.style.display === 'block') ? 'none' : 'block';
return true;
}

function ensureMessagePre(id, message) {
var pre = document.getElementById(id);
if (pre) 
return pre;



pre = document.createElement('pre');
pre.id = id;
pre.className = 'dev-debug-pre';
pre.style = 'display:block; white-space:pre-wrap; max-height:30vh; overflow:auto; background:#f6f8fa; padding:0.5rem; border:1px solid #ddd; color:#000;';
pre.textContent = message;
document.body.appendChild(pre);
return pre;
}

function init() { // floating panel (always visible when show_dev)
var panel = document.createElement('div');
panel.className = 'dev-debug-panel';
panel.setAttribute('role', 'region');
panel.setAttribute('aria-label', 'Dev debug panel');

var btnNav = document.createElement('button');
btnNav.className = 'dev-debug-toggle';
btnNav.textContent = 'Nav JSON';
btnNav.type = 'button';
btnNav.tabIndex = 0;
panel.appendChild(btnNav);

var btnProd = document.createElement('button');
btnProd.className = 'dev-debug-toggle';
btnProd.textContent = 'Products JSON';
btnProd.type = 'button';
btnProd.tabIndex = 0;
panel.appendChild(btnProd);

var info = document.createElement('span');
info.className = 'info';
info.textContent = 'dev mode';
panel.appendChild(info);

document.body.appendChild(panel);

// Expose a global function so clicks work even if addEventListener is blocked
window.__devToggle = function (id) {
var pre = document.getElementById(id);
if (! togglePre(pre)) {
if (id === 'devNavDebugJson') {
ensureMessagePre(id, 'No nav debug data (analysis.groupedTree missing or empty). Use the debug commands or ensure the controller provides analysis.groupedTree.');
} else {
ensureMessagePre(id, 'No products debug data (analysis.products / categoryProducts / items missing or empty).');
}
}
};

// Prefer addEventListener but also set onclick to the global function for robustness
try {
btnNav.addEventListener('click', function () {
window.__devToggle('devNavDebugJson');
});
} catch (e) {}
try {
btnProd.addEventListener('click', function () {
window.__devToggle('devProductsDebugJson');
});
} catch (e) {}

// Ensure onclick attributes exist (works even when addEventListener is disabled)
btnNav.setAttribute('onclick', "window.__devToggle('devNavDebugJson')");
btnProd.setAttribute('onclick', "window.__devToggle('devProductsDebugJson')");
}

if (document.readyState === 'loading') {
document.addEventListener('DOMContentLoaded', init);
} else {
init();
}
})();
</script>{% endif %}
