				{# Drei feste Footer-Container in definierter Reihenfolge: nur diese drei Spalten werden im Footer angezeigt
																																																																																                   Verwende die debug-Grouping-Logik (grouped by Modul) für konsistente Ergebnisse. #}
				{% set groups = footer_groups(0) %}
				{% set footerCols = ['SERVICE & HILFE', 'SHOP ON TOUR', 'CHEERCITY.SHOP'] %}

				{% for colName in footerCols %}
					<div class="col-lg-3 col-md-6 col-sm-6">
						<div class="footer-widget p-2">
							<div class="card-body p-0">
								<h6 class="footer-widget-title mb-2">{{ colName }}</h6>
								<ul class="widget-items cata-widget list-unstyled small">
									{% set items = groups[colName] is defined ? groups[colName] : [] %}
									{% if items is empty %}
										<li class="widget-list-item text-muted">Keine Einträge</li>
									{% else %}
										{% for item in items %}
											<li
												class="widget-list-item">
												{# Prefer explicit external link field `ext_lnk` when present #}
												{% set ext = item.ext_lnk ?? item.extLnk ?? item['ext_lnk'] ?? item['Ext_Lnk'] ?? '' %}
												{# Use alias from FM record to build internal service path when no external link #}
												{% set alias = (item.alias ?? item.Alias ?? item['alias'] ?? item['Alias'] ?? '')|trim %}
												{% if ext is not empty %}
													{% set href = ext %}
												{% elseif alias is not empty %}
													{% set href = path('servicer', {'slug': alias}) %}
												{% else %}
													{# Fallback to legacy link fields if neither ext nor alias present #}
													{% set href = item.lnk ?? item.link ?? item.cat_link ?? '' %}
												{% endif %}

												{% set title = item.titel ?? item.Titel ?? item['titel'] ?? '(ohne Titel)' %}
												{% if href is not empty %}
													<a href="{{ href }}" {% if href starts with('http') %} target="_blank" rel="noopener" {% endif %}>{{ title }}</a>
												{% else %}
													<span>{{ title }}</span>
												{% endif %}
											</li>
										{% endfor %}
									{% endif %}
								</ul>
							</div>
						</div>
					</div>
				{% endfor %}

