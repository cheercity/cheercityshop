			<div class="banner-inner">
				<div class="row">
					<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
						<div class="slider-inner">
							<div class="swiper rts-topSlide2">
								<div class="swiper-wrapper">
									{% if banners is defined and banners is not empty %}
										{% for banner in banners %}
											<div class="swiper-slide">
												<div class="banner-single">
													<div class="container header-container">
														<div class="single-inner">

															<div class="slider-image">
																{% if banner.source %}
																	{# If source looks like a video link, you might embed; here we render as image/video fallback to image tag #}
																	{% if banner.video_lnk is defined and banner.video_lnk %}
																		<video controls muted loop style="max-width:100%;height:auto;"><source src="{{ banner.video_lnk }}"></video>
																	{% else %}
																		<img src="{{ banner.source }}" alt="{{ banner.alt_tag ?: '' }}">
																	{% endif %}

																{% endif %}
															</div>
														</div>
													</div>
												</div>
											</div>
										{% endfor %}
									{% endif %}
								</div>
							</div>
							<div class="swiper-pag"></div>
						</div>
					</div>
				</div>
			</div>
		</div>
