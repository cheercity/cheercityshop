### Default environment variables for other servers (non-secret placeholders)
APP_ENV=prod
APP_SECRET=please_change_me

# Database (example)
DATABASE_URL="postgresql://app:password@127.0.0.1:5432/app?serverVersion=16&charset=utf8"

